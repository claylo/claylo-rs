{
  "name": "@{{ owner }}/{{ project_name }}",
  "version": "0.1.0",
  "description": "{{ project_description }}",
  "license": "{% if license | length > 1 %}({{ license | join(' OR ') }}){% else %}{{ license | first }}{% endif %}",
  "repository": {
    "type": "git",
    "url": "https://github.com/{{ owner }}/{{ project_name }}"
  },
  "bin": {
    "{{ project_name }}": "cli.js"
  },
  "scripts": {
    "postinstall": "node install.js"
  },
  "files": [
    "index.js",
    "cli.js",
    "install.js"
  ],
  "optionalDependencies": {
    "@{{ owner }}/{{ project_name }}-darwin-arm64": "0.1.0",
    "@{{ owner }}/{{ project_name }}-darwin-x64": "0.1.0",
    "@{{ owner }}/{{ project_name }}-linux-arm64": "0.1.0",
    "@{{ owner }}/{{ project_name }}-linux-x64": "0.1.0",
    "@{{ owner }}/{{ project_name }}-win32-arm64": "0.1.0",
    "@{{ owner }}/{{ project_name }}-win32-x64": "0.1.0"
  }
}
